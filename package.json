{
  "name": "serverless-server-spawner",
  "version": "1.0.15",
  "scripts": {
    "build": "rm -rf lib && npm run format && mkdir lib && cp -rv src/public lib/public && tsc -p .",
    "format": "prettier --write .",
    "release": "npm run build && npm version patch && npm publish && git push --follow-tags",
    "start-mock": "wiremock --root-dir test/wiremock --port 8081",
    "start-sss": "npm run build  && node ./lib/spawner/bin.js  --matchers-folder `pwd`/../serverless-sandbox/matchers  --time-to-live 1 --bitbucket-server-url http://localhost:8081 --bitbucket-server-access-token whatever --bitbucket-server-projects PRJ1,PRJ2 --minimum-port-number 9010 --maximum-port-number 9012",
    "start": "concurrently \"npm run start-mock\" \"npm run start-sss\" --kill-others"
  },
  "bin": "./lib/spawner/bin.js",
  "dependencies": {
    "axios": "^0.24.0",
    "commander": "^8.3.0",
    "express": "^4.17.1",
    "figlet": "^1.5.2",
    "fs-extra": "^10.0.0",
    "node-cache": "^5.1.2",
    "path": "^0.12.7",
    "portastic": "^1.0.1",
    "tree-kill": "^1.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^16.9.1",
    "@types/uuid": "^8.3.3",
    "concurrently": "^7.0.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "typescript": "^4.5.4",
    "wiremock": "^2.32.0"
  },
  "homepage": "https://github.com/tomasbjerre/serverless-server-spawner",
  "repository": "git@github.com:tomasbjerre/serverless-server-spawner.git",
  "files": [
    "lib"
  ],
  "author": "Tomas Bjerre",
  "keywords": [
    "serverless"
  ],
  "license": "Apache 2"
}
